// Narrativa
salarioLiquido = ( SalarioBruto - Descontos + Comissão )

1- Crie 4 variaveis tipo decimal (salarioBruto, desconto, comissao e salarioLiquido)
2- Receba valores salarioBruto, desconto e comissao
3.1- Faça cálculo do salário líquido, sendo
3.2- salarioLiquido = (salarioBruto - desconto + comissão)
4- Exiba salarioLiquido

//Pseudocódigo

algoritmo "Salário Líquido"

declare
	salarioBruto, desconto, comissao : decimal
	salarioLiquido : decimal

inicio
 	
    escreva("Insira seu salário bruto:")
	leia(salarioBruto)
	
    escreva("Insira a porcentagem do desconto do salário bruto:")
	leia(desconto)

    escreva("Insira a comissão do salário:")
	leia(comissao)

	salarioLiquido = (salarioBruto - desconto + comissão)
	
    escreva("Média = ", media)

fim

//Código

int main (){
    
    float salarioBruto, desconto, comissao;
    float salarioLiquido;

    printf("Insira seu salário bruto:");
    scanf("%f", &salarioBruto);

    printf("Insira a porcentagem do desconto do salário bruto:");
    scanf("%f", &desconto);

    printf("Insira a comissão do salário:");
    scanf("%f", &comissão);

    salarioLiquido = (salarioBruto - desconto + comissão);

    printf("Salário Líquido= %f", salarioLiquido);
}
